      "\\.(css)$": "<rootDir>/test/__mocks__/cssMock.js"
    },
    "setupFiles": [
      "<rootDir>/test/raf-shim.js",
      "<rootDir>/test/setup.js"
    ],
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    }
  },
